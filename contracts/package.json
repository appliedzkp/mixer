{
  "name": "mixer-contracts",
  "version": "1.0.0",
  "description": "",
  "main": "build/index.js",
  "scripts": {
    "build": "tsc && ./scripts/buildSolidity.sh",
    "build-tsc": "tsc",
    "test": "etherlime test --path=build/__tests__ --skip-compilation --timeout 90000",
    "test-debug": "node --inspect-brk ./node_modules/.bin/etherlime test --path=build/__tests__ --skip-compilation --timeout 90000",
    "test-mix-tokens": "etherlime test --path=build/__tests__/TokenMixer.test.js --skip-compilation --timeout 600000",
    "test-mix-tokens-debug": "node --inspect-brk ./node_modules/.bin/etherlime test --path=build/__tests__/TokenMixer.test.js --skip-compilation --timeout 600000",
    "test-mix-stress": "etherlime test --path=build/__tests__/MixerStress.test.js --skip-compilation --timeout 5000000",
    "test-mix-stress-debug": "node --inspect-brk ./node_modules/.bin/etherlime test --path=build/__tests__/MixerStress.test.js --skip-compilation --timeout 5000000",
    "watch": "tsc --watch",
    "deploy": "./scripts/deploy.sh",
    "compile": "tsc && node ./build/deploy/compile.js",
    "ganache": "./scripts/runGanache.sh"
  },
  "author": "Koh Wei Jie",
  "license": "GPL-3.0-or-later",
  "_moduleAliases": {
    "@mixer-contracts": "."
  },
  "devDependencies": {
    "@types/jest": "^26.0.20",
    "@types/node": "^14.14.31",
    "@types/lodash": "^4.14.168",
    "argparse": "^2.0.1",
    "circomlib": "0.4.1",
    "module-alias": "^2.2.2",
    "ethers": "^4",
    "ganache-cli": "^6.12.2",
    "ganache-core": "^2.13.2",
    "solc": "^0.5.17",
    "truffle-artifactor": "^4.0.10"
  },
  "dependencies": {
    "config": "^3.3.3",
    "mixer-config": "1.0.0",
    "libsemaphore": "^1.0.16",
    "mixer-utils": "1.0.0",
    "openzeppelin-solidity": "^3.4.0"
  }
}
